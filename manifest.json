{
  "manifest_version": 2,

  "name": "ytv",
  "description": "YouTube TV for Browser",
  "version": "1.0",

  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*.youtube.com/tv*"
  ],

  "background": {
    "scripts": [
      "index.js"
    ]
  },
  "browser_action": {
    "default_title": "ytv"
  }
}
